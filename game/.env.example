# Copy to .env.local and fill in your Supabase project credentials.
# If left empty, scores will be stored in localStorage as a fallback.

VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Socket.io server URL
# Development default (local server):
VITE_SOCKET_URL=http://localhost:8080
# Production: set to your deployed server URL, e.g.:
# VITE_SOCKET_URL=https://your-server.example.com

# Admin panel secret (must match ADMIN_SECRET on the server)
VITE_ADMIN_SECRET=admin1234
